<Window x:Class="PCGaurdianV1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PCGaurdianV1"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="600">
    <Window.Background>
        <ImageBrush ImageSource="Images/bg_3.jpg"/>
    </Window.Background>

    <Grid HorizontalAlignment="Center" VerticalAlignment="Center">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <WrapPanel HorizontalAlignment="Center" Height="350" Width="500">
            <Border Background="#FF558D4A" CornerRadius="100">
                <Frame x:Name="frame1" HorizontalAlignment="Left" Height="380" VerticalAlignment="Top" Width="517" NavigationUIVisibility="Hidden" Margin="0,0,0,-11" Background="#FF1CAA9D"/>
            </Border>
        </WrapPanel>

    </Grid>
</Window>
